{
  "manifest_version": 2,
  "author": "Michael Ziminsky",

  "name": "Facebook Tracking Removal",
  "version": "1.1.0",
  "description": "Removes Facebook's tracking from external links and videos",
  "permissions": [
    "storage",
    "*://*.facebook.com/*"
  ],

  "options_ui": {
    "page": "options.html"
  },

  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*"],
      "js": ["util.js", "app.js"],
      "run_at": "document_end"
    }
  ]
}
